{
  "name": "tabxport-extension",
  "displayName": "TabXport",
  "version": "0.2.3",
  "description": "Export tables from AI chat platforms (ChatGPT, Claude, Gemini, DeepSeek) to Excel/CSV with intelligent filename generation",
  "author": "TabXport Team",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "typecheck": "tsc --noEmit",
    "lint": "prettier --check ."
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "cheerio": "^1.0.0",
    "clean-text-utils": "^1.1.17",
    "node-html-parser": "^7.0.1",
    "normalize-text": "^2.6.0",
    "parse-markdown-table": "^0.0.6",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "^0.0.258",
    "@types/node": "^20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://chat.openai.com/*",
      "https://claude.ai/*",
      "https://gemini.google.com/*",
      "https://chat.deepseek.com/*",
      "https://deepseek.com/*",
      "https://www.googleapis.com/*",
      "file:///*"
    ],
    "permissions": [
      "storage",
      "downloads",
      "scripting",
      "notifications",
      "identity"
    ]
  }
}
